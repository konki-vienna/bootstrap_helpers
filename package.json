{
  "name": "sketch-bootstrap-helpers",
  "version": "1.1.9",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "sketch-bootstrap-helpers",
    "manifest": "src/manifest.json",
    "main": "sketch-bootstrap-helpers.sketchplugin",
    "appcast": "appcast.xml",
    "assets": [
      "assets/**/*"
    ]
  },
  "assets": [
    "assets/**/*"
  ],
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "0.7.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/konki-vienna/sketch-bootstrap-helpers.git"
  },
  "license": "MIT",
  "author": "Konstantin Demblin <konstantin.demblin@beeone.at>",
  "dependencies": {
    "sketch-module-google-analytics": "0.2.2"
  }
}
